{
  "name": "evs-microservice",
  "version": "2.0.1",
  "description": "Microservice for the management of electric vehicles (EVs).",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:watch": "jest --watch",
    "test:pipeline": "jest --coverage",
    "start": "nodemon app.js",
    "start:prod": "node app.js",
    "start:local": "rm -rf node_modules; npm i; npm run start;",
    "start:local:win": "rmdir /s node_modules; npm i; npm run start;"
  },
  "overrides": {
    "glob": "^10.4.5",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "json-schema-ref-parser": "^9.0.9",
    "text-encoding": "0.7.0",
    "swagger-methods": "2.0.2",
    "axios": "^1.12.2"
  },
  "author": "EVIO",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^14.2.1",
    "@sentry/node": "^7.68.0",
    "@sentry/profiling-node": "^1.2.1",
    "axios": "^1.12.2",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dns-prefetch-control": "^0.3.0",
    "dotenv-safe": "^6.1.0",
    "evio-library-assets": "^1.0.2",
    "evio-library-commons": "^1.0.0",
    "evio-library-evs": "^1.0.0",
    "evio-library-identity": "^1.0.3",
    "evio-redis-connection": "^1.0.0",
    "evio-toggle": "^1.0.0",
    "expect-ct": "^1.0.0",
    "express": "^4.17.1",
    "frameguard": "^4.0.0",
    "helmet": "^3.18.0",
    "http": "0.0.0",
    "ienoopen": "^1.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongoose": "^5.13.9",
    "morgan": "^1.9.1",
    "node-cron": "^3.0.2",
    "node-schedule": "^2.1.1",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.1.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "eslint": "^9.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "PORT": 3006,
      "PORT_DEV": 3006
    }
  }
}
