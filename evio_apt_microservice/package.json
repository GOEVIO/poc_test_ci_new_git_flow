{
  "name": "evio_apt_microservice",
  "version": "0.0.6",
  "description": "",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "nest build",
    "postbuild": "cp package*.json build && cd build &&  npm ci --omit=dev",
    "start:pre": "npm run delete; nest start",
    "start:local": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node build/src/main",
    "lint": "eslint '{src,test}/**/*.ts'",
    "lint:fix": "eslint '{src,test}/**/*.ts' --fix",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "test:watch": "jest --watch",
    "test:pipeline": "jest",
    "reset": "rm -rf node_modules; rm -rf dist; npm i; npm run build; npm run start:dev",
    "test": "jest",
    "typeorm": "APP=migration ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm -d ./src/database/datasource.ts",
    "migration:create": "read name && npx typeorm-ts-node-commonjs migration:create ./src/database/migrations/$name",
    "migration:run": "APP=migration npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@fastify/static": "^8.2.0",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-fastify": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@sentry/node": "^10.0.0",
    "@sentry/tracing": "^7.102.0",
    "@types/dotenv-safe": "^8.1.6",
    "amqplib": "^0.10.9",
    "axios": "^1.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv-safe": "^9.1.0",
    "evio-library-chargers": "file:../local-npm-packages/evio-library-chargers/dist",
    "evio-library-commons": "file:../local-npm-packages/evio-library-commons/dist",
    "evio-library-configs": "file:../local-npm-packages/evio-library-configs/dist",
    "evio-library-connections": "file:../local-npm-packages/evio-library-connections/dist",
    "evio-library-identity": "file:../local-npm-packages/evio-library-identity/dist",
    "evio-library-ocpi": "file:../local-npm-packages/evio-library-ocpi/dist",
    "evio-library-payments": "file:../local-npm-packages/evio-library-payments/dist",
    "pg": "^8.16.3",
    "reflect-metadata": "0.2.1",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "typeorm": "^0.3.25",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.0",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "20.8.7",
    "@types/supertest": "2.0.16",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitest/coverage-v8": "^1.5.3",
    "eslint": "8.42.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "supertest": "6.3.3",
    "ts-jest": "^29.1.5",
    "ts-loader": "9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.3"
  }
}
