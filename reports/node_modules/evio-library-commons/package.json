{
  "name": "evio-library-commons",
  "version": "1.0.0",
  "publishConfig": {
    "registry": "http://54.216.85.4:8081/repository/evio-npm/"
  },
  "description": "A simple feature toggle utility for Node.js using Flagsmith for remote feature flag management.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist; tsc --experimentalDecorators --module commonjs",
    "build:win": "rmdir /s dist; tsc --experimentalDecorators --module commonjs",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:watch": "jest --watch",
    "test:pipeline": "jest",
    "start": "nodemon index.ts",
    "start:local": "rm -rf node_modules; npm i; npm run build; npm run start;",
    "start:local:win": "rmdir /s node_modules; npm i; npm run build:win; npm run start;",
    "prepublishOnly": "npm run build && npm pack --dry-run"
  },
  "overrides": {
    "glob": "^10.4.5"
  },
  "keywords": [
    "commons"
  ],
  "author": "",
  "contributors": [],
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^9.17.0",
    "dotenv": "^17.2.2",
    "http-status-codes": "^2.3.0",
    "soap": "^1.1.11",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "eslint": "^9.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
